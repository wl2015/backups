.add-admin {
	position: absolute;
	z-index: 10010;
	top: 64px; 
	left: 17.5%; 
	background: none repeat scroll 0 0 rgba(40, 40, 40, 0.6);
	height: 1104px;
	width: 82.5%;
}
.add-admin .add-user-content {
	height: 924px; 
	width: 640px;
	margin: 30px 30%;
}
.add-admin .add-user-header, .extend-title {
	background: none repeat scroll 0 0 #3D9970; 
	border-radius: 2px 2px 0 0; 
	box-shadow: 0 -2px 0 rgba(0, 0, 0, 0.2) inset; 
	color: #FFFFFF; 
	font-size: 26px; 
	font-weight: 300; 
	padding: 15px 5px; 
	text-align: center;
}
.add-admin .footer {
	border-radius: 0 0 2px 2px;
}
.add-admin .body, .add_admin .footer {
    background: none repeat scroll 0 0 #FFFFFF;
    color: #EAEAEC;
    padding: 30px 20px 10px;
    width: 600px;
}
.add-admin .bg-gray {
    background-color: #EAEAEC !important;
}
.add-admin .form-group {
	margin-top: 5px;
    margin-bottom: 15px;
    display: inline-block;
    text-align: center;
    width: 600px;
}
.add-admin .btn {
	color: #FFFFFF;
	-moz-user-select: none;
    background-image: none;
    border: 1px solid rgba(0, 0, 0, 0);
    border-radius: 2px;
    cursor: pointer;
    display: inline-block;
    font-size: 14px;
    font-weight: normal;
    line-height: 1.42857;
    margin-bottom: 10px;
    padding: 6px 12px;
    text-align: center;
    vertical-align: middle;
    white-space: nowrap;
    width: 100%;
    background-color: #3D9970 !important;
    height: 40px;
}
.form-group .form-label {
	width: 180px;
	font-size: 14px;
    height: 39px;
    line-height: 34px;
	background-color: rgba(129, 183, 121, 1);
	display: inline-block;
}
.form-group .form-control {
	border: medium none #FFFFFF;
	background-color: #FFFFFF;
    background-image: none;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    color: #555555;
    font-size: 14px;
    height: 34px;
    padding: 3px 12px;
    transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
    vertical-align: middle;
    width: 380px;
    border-radius: 0 !important;
    margin-left: -4px;
}
.form-control::-moz-placeholder {
    color: #999999;
    opacity: 1;
}
.form-group .form-control-er {
	border: 1px solid rgba(241, 0, 0, 0.75);
    box-shadow: 0 0 3px rgba(241, 0, 0, 0.6);
    outline: medium none;
    transition: border 0.2s linear 0s, -moz-box-shadow 0.5s linear 0s;
}
.form-group .form-control-ac {
	display: inline-block;
	line-height: 34px;
	text-align: left;
	color: #444444;
}
.form-group .form-control-ac input {
	margin-left: 20px;
}

